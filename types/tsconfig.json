{
	"compilerOptions": {
		"declaration": true,
		"forceConsistentCasingInFileNames": true,
		"lib": ["ES2020"],
		"module": "CommonJS",
		"moduleResolution": "Node",
		"outDir": "dist/",
		"resolveJsonModule": true,
		"rootDir": "lib/",
		"target": "ES2020",
		"downlevelIteration": true,
		"esModuleInterop": true,
		"strictNullChecks": true
	},
	"exclude": ["dist/"],
	"include": ["lib/"]
}